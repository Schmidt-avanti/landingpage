{
  "cleanUrls": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/datenschutz", "destination": "/datenschutz.html" },
    { "source": "/preise", "destination": "/preise.html" },
    { "source": "/impressum", "destination": "/impressum.html" }
  ],
  "routes": [
    { "src": "/datenschutz", "dest": "/datenschutz.html" },
    { "src": "/preise", "dest": "/preise.html" },
    { "src": "/impressum", "dest": "/impressum.html" }
  ]
}
